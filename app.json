{
  "expo": {
    "name": "servify",
    "description": "Easy way to find local services brougth to your home, contact them and make your life easier ",
    "slug": "servify",
    "privacy": "public",
    "sdkVersion": "28.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/servifyIcon.png",
    "splash": {
      "image": "./assets/servifySplashOrange.png",
      "resizeMode": "cover",
      "backgroundColor": "#FF7043"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jorgebaralt.servify"
    },
    "android": {
      "package": "com.jorgebaralt.servify"
    }
  }
}
